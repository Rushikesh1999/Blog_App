<% include ./partials/header %>

<% blogs.forEach(function(blog){%>
    <div>
        <h2><%= blog.title%></h2>
        <img style: width=200 height=200 src="<%= blog.image%>">
        <span><%= blog.created.toDateString() %></span>
        <p><%= blog.body %></p>
        <a href="/blogs/<%= blog._id%>">Read More</a>
    </div>
<% })%>    

<% include ./partials/footer %>